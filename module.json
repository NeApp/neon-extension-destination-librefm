{
  "title": "Libre.fm",

  "origins": [
    "*://libre.fm/*"
  ],

  "services": [
    "Configuration",
    "Destination:Scrobble"
  ],
  "web_accessible_resources": [
    "destination/librefm/callback/callback.html"
  ],

  "webpack": {
    "babel": [
      "node_modules/@fuzeman/librefm/src"
    ],
    "modules": {
      "Callback": true
    }
  }
}
